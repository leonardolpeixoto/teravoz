{
  "name": "teravoz",
  "version": "1.0.0",
  "description": "full stack application",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "nodemon src/server.js --exec babel-node",
    "build": "babel src/ -d dist",
    "server": "npm run build && pm2 start ./dist/server.js",
    "lint": "eslint --fix src/*.js",
    "stop-server": "pm2 stop ./dist/server.js",
    "test-integration": "NODE_ENV=test mocha --opts tests/mocha.opts tests/integration/**/*.js",
    "test-unit": "NODE_ENV=test mocha --opts tests/mocha.opts tests/unit/**/*.js",
    "doc": "esdoc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leonardolpeixoto/teravoz.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/leonardolpeixoto/teravoz/issues"
  },
  "homepage": "https://github.com/leonardolpeixoto/teravoz#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.0",
    "mocha": "^5.0.5",
    "nodemon": "^1.18.4",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.14.0",
    "express-jwt": "^5.3.1",
    "helmet": "^3.13.0",
    "http-status": "^0.2.3",
    "jsonwebtoken": "^8.2.0",
    "jwt-simple": "^0.5.0",
    "log4js": "^3.0.5",
    "morgan": "^1.9.0",
    "mysql2": "^1.6.1",
    "passport": "^0.3.2",
    "passport-jwt": "^2.1.0",
    "pm2": "^2.10.2",
    "ramda": "^0.25.0",
    "sequelize": "^4.39.0",
    "socket.io": "^2.1.1"
  }
}
